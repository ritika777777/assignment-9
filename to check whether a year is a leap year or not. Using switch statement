#include<stdio.h>
int main()

{
    int year, num;

printf("enter year");
scanf("%d",&year);

num=((year%4==0)&&((year%400==0)||(year%100!=0)));
 switch(num)
 {
    case(0):
    printf("not leap year");
    break;

    case(1):
    printf("leap year");
    break;

    default:
    printf("anonymas");
    break;
   }
  return 0;
}
