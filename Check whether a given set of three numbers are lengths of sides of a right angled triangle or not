#include<stdio.h>
int main()

{
    int x,y,z,flag=0;
    printf("enter three sides of triangele");
    scanf("%d%d%d",&x,&y,&z);
    if((x*x)+(y*y)==(z*z) || (x*x)+(z*z)==(y*y) || (y*y)+(z*z)==(x*x)){

    flag=1;
    }
    else{
        flag=0;
    }
 switch(flag)
 {
 case(0):
    printf("It is not right angle triangle");
    break;

   case(1):
     printf("It is a right angle triangle");
      break;

   default:
    printf("anonymous");

   }
  return 0;
}
