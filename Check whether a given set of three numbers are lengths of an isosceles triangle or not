#include<stdio.h>
int main()

{
    int a,b,c,flag=0;
    printf("enter three sides of triangele");
    scanf("%d%d%d",&a,&b,&c);
    if(a==b || b==c || c==a){
    flag=1;
    }
    else{
        flag=0;
    }
 switch(flag)
 {
 case(0):
    printf("It is not isosceles triangle");
    break;

   case(1):
     printf("It is an isosceles triangle");
      break;

   default:
    printf("anonymous");

   }
  return 0;
}
