int main()
{
    int a,b,c;
    printf("enter the value of a,b and c");
    scanf("%d%d%d",&a,&b,&c);

    switch(b*b-4*a*c>0){
        case(1):
       printf("distinct and real roots ");
     break;

    case 0:
        switch(b*b-4*a*c==0){
        case(1):
        printf("equal and real roots");
        break;
        case(0):
            printf("imaginary");
            break;
    default:
        printf("anonymos");
        }
    getch();
}
}
